{
  "expo": {
    "name": "MDRRMO",
    "slug": "mdrmmoadmin",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "mdrrmo",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "extra": {
      "baseUrl": "http://192.168.43.40:3000",
      "eas": {
        "projectId": "baa9c7e7-a90b-474d-9479-3ab56009f85e"
      }
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "owner": "uranullo"
  }
}